
pipeline {
    agent any
    stages {
        stage('fetch code') {
            steps {
                sh 'mvn --version'
            }
        }
        stage('build code'){
            steps{
                sh'ls'
            }
        }
          stage('run tests'){
            steps{
                sh'ls'
            }
        }
           stage('Publish test results'){
            steps{
                sh'ls'
            }
        }
          stage('Deploy war files'){
            steps{
                sh'ls'
            }
        }
    }
}
